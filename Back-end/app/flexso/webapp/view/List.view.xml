<mvc:View
	controllerName="flexso.controller.List"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<f:DynamicPage id="dynamicPageId" toggleHeaderOnTitleClick="false">
		<!-- DynamicPage Title -->
	

		<!-- DynamicPage Content -->
		<f:content>
			<VBox id="_IDGenVBox1" fitContainer="true">
				<Table
					id="eventsTable"
					inset="false"
					items="{/Events}"
					class="sapFDynamicPageAlignContent"
					width="auto">
					<columns>
						<Column id="_IDGenColumn1" width="12em">
							<Text id="_IDGenText1" text="Product"/>
						</Column>
						<Column id="_IDGenColumn2"	hAlign="End">
							<Text id="_IDGenText2" text="Price"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem id="_IDGenColumnListItem1" type="Navigation" press=".onListItemPress">
							<cells>
								<ObjectIdentifier id="_IDGenObjectIdentifier1" title="{naam}" text="{eventID}"/>

							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</VBox>
		</f:content>

		

	</f:DynamicPage>
</mvc:View>